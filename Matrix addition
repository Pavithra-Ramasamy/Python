r=int(input())
c=int(input())
a=[]
b=[]
add=[]
for i in range(0,r):
    x=[]
    for j in range(0,r):
        n=int(input())
        x.append(n)
    a.append(x)    
for i in range(0,r):
    y=[]
    for j in range(0,r):
        n1=int(input())
        y.append(n1)
    b.append(y) 
for i in range(0,r):
    s=[]
    for j in range(0,r):
        sum=a[i][j]+b[i][j]
        s.append(sum)
    add.append(s)
print(add)  
